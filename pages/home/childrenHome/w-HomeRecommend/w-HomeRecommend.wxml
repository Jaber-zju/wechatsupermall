<!--pages/home/childrenHome/w-HomeRecommend/w-HomeRecommend.wxml-->
<view class="recommend">
  <block wx:for="{{list}}" wx:key="{{index}}">
    <view class="recommend-item">
      <image class="item-image" src="{{item.image}}" bindload="imageLoad"></image>
      <view>{{item.title}}</view>
    </view>
  </block>
</view>   